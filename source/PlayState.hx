			BF_X2 = stageData.bf2[0];
			BF_Y2 = stageData.bf2[1];
			BF_X3 = stageData.bf3[0];
			BF_Y3 = stageData.bf3[1];
			BF_X4 = stageData.bf4[0];
			BF_Y4 = stageData.bf4[1];
			BF_X5 = stageData.bf5[0];
			BF_Y5 = stageData.bf5[1];
		}

		if(stageData.camera_speed != null)
			cameraSpeed = stageData.camera_speed;

		boyfriendCameraOffset = stageData.camera_boyfriend;
		if(boyfriendCameraOffset == null) //Fucks sake should have done it since the start :rolling_eyes:
			boyfriendCameraOffset = [0, 0];

		opponentCameraOffset = stageData.camera_opponent;
		if(opponentCameraOffset == null)
			opponentCameraOffset = [0, 0];

		girlfriendCameraOffset = stageData.camera_girlfriend;
		if(girlfriendCameraOffset == null)
			girlfriendCameraOffset = [0, 0];

		dad_cameraoffset = stageData.dad_offset;
		if(dad_cameraoffset == 0)
			dad_cameraoffset = 30;

		bf_cameraoffset = stageData.bf_offset;
		if(bf_cameraoffset == 0)
			bf_cameraoffset = 30;

		boyfriendGroup = new FlxSpriteGroup(BF_X, BF_Y);

		if (melatonin)
		{
			boyfriendGroup2 = new FlxSpriteGroup(BF_X2, BF_Y2);
			boyfriendGroup3 = new FlxSpriteGroup(BF_X3, BF_Y3);
			boyfriendGroup4 = new FlxSpriteGroup(BF_X4, BF_Y4);
